<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
    <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <tr>
    <% @customers.each do |customer| %>
    <td><%= customer.id %></td>
    <td><%= customer.last_name %></td>
    <td><%= customer.email %></td>
    <td><%= customer.is_deleted %></td>
    <td><%= link_to "", admin_customer_path(customer.id) %></td>
    <% end %>
  </tr>
  </tbody>
</table>